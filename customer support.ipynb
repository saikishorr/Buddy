   "source": [
    "# =====================================\n",
    "# 1️⃣ Import Required Libraries\n",
    "# =====================================\n",
    "!pip install langchain langchain_core langchain_groq langchain_community langgraph gradio pandas\n",
    "\n",
    "from typing import TypedDict, Dict\n",
    "from langgraph.graph import StateGraph, END\n",
    "from langchain_core.prompts import ChatPromptTemplate\n",
    "from langchain_core.runnables.graph import MermaidDrawMethod\n",
    "from IPython.display import display, Image\n",
    "import pandas as pd\n",
    "import os\n",
    "\n",
    "# =====================================\n",
    "# 2️⃣ Define State Class\n",
    "# =====================================\n",
    "class State(TypedDict):\n",
    "    query: str\n",
    "    category: str\n",
    "    sentiment: str\n",
    "    response: str\n",
    "\n",
    "\n",
    "# =====================================\n",
    "# 3️⃣ Connect to ChatGroq (LLaMA Model)\n",
    "# =====================================\n",
    "from langchain_groq import ChatGroq\n",
    "\n",
    "llm = ChatGroq(\n",
    "    temperature=0,\n",
    "    groq_api_key=\"gsk_",  # your key\n",
    "    model_name=\"llama-3.3-70b-versatile\"\n",
    ")\n",
    "# =====================================\n",
    "# 4️⃣ Load and Combine CSV and TXT Dataset Files\n",
    "# =====================================\n",
    "import json\n",
    "\n",
    "# Specify the folder where all your datasets are stored\n",
    "dataset_folder = \"datasets/\"  # 👉 change this to your folder path\n",
    "\n",
    "# Read all CSV and TXT files dynamically\n",
    "dataframes = []\n",
    "txt_data = []\n",
    "\n",
    "for file in os.listdir(dataset_folder):\n",
    "    filepath = os.path.join(dataset_folder, file)\n",
    "    if file.endswith(\".csv\"):\n",
    "        df = pd.read_csv(filepath)\n",
    "        dataframes.append(df)\n",
    "    elif file.endswith(\".txt\"):\n",
    "        # Read JSON from TXT\n",
    "        with open(filepath, \"r\", encoding=\"utf-8\") as f:\n",
    "            try:\n",
    "                data = json.load(f)  # Expecting JSON list of dicts\n",
    "                txt_data.extend(data)\n",
    "            except json.JSONDecodeError:\n",
    "                print(f\"⚠️ Failed to parse {file}, skipping.\")\n",
    "\n",
    "# Combine all CSV datasets into one\n",
    "if dataframes:\n",
    "    combined_df = pd.concat(dataframes, ignore_index=True)\n",
    "    print(\"✅ Loaded CSV dataset with shape:\", combined_df.shape)\n",
    "else:\n",
    "    combined_df = pd.DataFrame()\n",
    "    print(\"⚠️ No CSV files found in the dataset folder.\")\n",
    "\n",
    "# Convert TXT JSON data to DataFrame\n",
    "if txt_data:\n",
    "    txt_df = pd.DataFrame(txt_data)\n",
    "    print(\"✅ Loaded TXT dataset with shape:\", txt_df.shape)\n",
    "else:\n",
    "    txt_df = pd.DataFrame()\n",
    "    print(\"⚠️ No TXT dataset found.\")\n",
    "\n",
    "# Merge CSV + TXT datasets for searching\n",
    "if not combined_df.empty and not txt_df.empty:\n",
    "    combined_df = pd.concat([combined_df, txt_df], ignore_index=True)\n",
    "elif combined_df.empty:\n",
    "    combined_df = txt_df\n",
    "\n",
    "print(\"📊 Final combined dataset shape:\", combined_df.shape)\n",
    "\n",
    "# =====================================\n",
    "# 5️⃣ Helper Function — Search Dataset (Updated)\n",
    "# =====================================\n",
    "def search_dataset(query: str) -> str:\n",
    "    \"\"\"Search inside CSV/TXT combined dataset.\"\"\"\n",
    "    if combined_df.empty:\n",
    "        return \"Dataset is empty or not loaded properly.\"\n",
    "\n",
    "    query_lower = query.lower()\n",
    "    results = combined_df[\n",
    "        combined_df.apply(lambda row: row.astype(str).str.lower().str.contains(query_lower).any(), axis=1)\n",
    "    ]\n",
    "\n",
    "    if results.empty:\n",
    "        return \"No relevant data found in the dataset.\"\n",
    "    else:\n",
    "        # Convert the first few rows to readable text\n",
    "        return results.head(5).to_string(index=False)\n",
    "\n",
    "\n",
    "\n",
    "# =====================================\n",
    "# 6️⃣ Define Workflow Nodes\n",
    "# =====================================\n",
    "def categorize(state: State) -> State:\n",
    "    \"\"\"Categorize the query.\"\"\"\n",
    "    prompt = ChatPromptTemplate.from_template(\n",
    "        \"Categorize the following customer query into one of these categories: \"\n",
    "        \"Technical, Billing, General, or DataSearch. Query: {query}\"\n",
    "    )\n",
    "    chain = prompt | llm\n",
    "    category = chain.invoke({\"query\": state[\"query\"]}).content\n",
    "    return {\"category\": category}\n",
    "\n",
    "\n",
    "def analyze_sentiment(state: State) -> State:\n",
    "    \"\"\"Analyze sentiment of the query.\"\"\"\n",
    "    prompt = ChatPromptTemplate.from_template(\n",
    "        \"Analyze the sentiment of the following customer query \"\n",
    "        \"Respond with either 'Positive', 'Neutral', or 'Negative'. Query: {query}\"\n",
    "    )\n",
    "    chain = prompt | llm\n",
    "    sentiment = chain.invoke({\"query\": state[\"query\"]}).content\n",
    "    return {\"sentiment\": sentiment}\n",
    "\n",
    "\n",
    "def handle_technical(state: State) -> State:\n",
    "    \"\"\"Handle technical queries.\"\"\"\n",
    "    prompt = ChatPromptTemplate.from_template(\n",
    "        \"Provide a technical support response to the following query: {query}\"\n",
    "    )\n",
    "    chain = prompt | llm\n",
    "    response = chain.invoke({\"query\": state[\"query\"]}).content\n",
    "    return {\"response\": response}\n",
    "\n",
    "\n",
    "def handle_billing(state: State) -> State:\n",
    "    \"\"\"Handle billing queries.\"\"\"\n",
    "    prompt = ChatPromptTemplate.from_template(\n",
    "        \"Provide a billing support response to the following query: {query}\"\n",
    "    )\n",
    "    chain = prompt | llm\n",
    "    response = chain.invoke({\"query\": state[\"query\"]}).content\n",
    "    return {\"response\": response}\n",
    "\n",
    "\n",
    "def handle_general(state: State) -> State:\n",
    "    \"\"\"Handle general queries.\"\"\"\n",
    "    prompt = ChatPromptTemplate.from_template(\n",
    "        \"Provide a general support response to the following query: {query}\"\n",
    "    )\n",
    "    chain = prompt | llm\n",
    "    response = chain.invoke({\"query\": state[\"query\"]}).content\n",
    "    return {\"response\": response}\n",
    "\n",
    "\n",
    "def handle_dataset(state: State) -> State:\n",
    "    \"\"\"Fetch data from CSV dataset if query is data-related.\"\"\"\n",
    "    query = state[\"query\"]\n",
    "    dataset_result = search_dataset(query)\n",
    "    return {\"response\": dataset_result}\n",
    "\n",
    "\n",
    "def escalate(state: State) -> State:\n",
    "    \"\"\"Escalate negative sentiment queries.\"\"\"\n",
    "    return {\"response\": \"This query has been escalated to a human agent due to its negative sentiment.\"}\n",
    "\n",
    "\n",
    "# =====================================\n",
    "# 7️⃣ Route Query Logic\n",
    "# =====================================\n",
    "def route_query(state: State) -> str:\n",
    "    \"\"\"Route query based on category and sentiment.\"\"\"\n",
    "    if state[\"sentiment\"] == \"Negative\":\n",
    "        return \"escalate\"\n",
    "    elif \"DataSearch\" in state[\"category\"]:\n",
    "        return \"handle_dataset\"\n",
    "    elif \"Technical\" in state[\"category\"]:\n",
    "        return \"handle_technical\"\n",
    "    elif \"Billing\" in state[\"category\"]:\n",
    "        return \"handle_billing\"\n",
    "    else:\n",
    "        return \"handle_general\"\n",
    "\n",
    "\n",
    "# =====================================\n",
    "# 8️⃣ Build the Workflow Graph\n",
    "# =====================================\n",
    "workflow = StateGraph(State)\n",
    "\n",
    "workflow.add_node(\"categorize\", categorize)\n",
    "workflow.add_node(\"analyze_sentiment\", analyze_sentiment)\n",
    "workflow.add_node(\"handle_technical\", handle_technical)\n",
    "workflow.add_node(\"handle_billing\", handle_billing)\n",
    "workflow.add_node(\"handle_general\", handle_general)\n",
    "workflow.add_node(\"handle_dataset\", handle_dataset)\n",
